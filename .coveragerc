[run]
branch = False
omit =
    papermill/tests/*
    papermill/_version.py

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    papermill/tests/*
    */site-packages/*
    papermill/_version.py
